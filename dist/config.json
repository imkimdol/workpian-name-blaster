  "flaggedNameExpressions": [
    "^(?!.*(Award|Cohort).*Name).*Name.*",
    "^Student$",
    "^Academic Record$"
  ],
  "flaggedOtherExpressions": [
    "Date of Birth",
    "Age",
    "Citizenship Status",
    "Trans Experience",
    "Gender",
    "Pronouns",
    "Address",
    "City",
    "State",
    "Country",
    "Postal Code",
    "Phone Number",
    "Identification #",
    "SIN",
    "Social Insurance Number",
    "Sex",
    "Contact Information"
  ]
}
