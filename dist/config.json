{
  "enableTableAlgorithm": true,
  "enableListAlgorithm": true,
  "enableSimpleTemplateAlgorithm": true,

  "useNumericDividerSubalgorithm": true,
  "useTextBeforeNumericDividerWorkday": true,
  "useTextBeforeNumericDividerAppian": false,

  "useColonDividerSubalgorithm": true,
