{
  "manifest_version": 3,
  "name": "WorkPian Name Blaster",
  "description": "Lorem ipsum dolor sit amet",
  "version": "0.9",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.myworkday.com/*"],
      "js": ["workday.js"]
    },
    {
      "matches": ["https://*.appiancloud.com/*"],
      "js": ["appian.js"]
    }
  ],
 "web_accessible_resources": [
    {
      "matches": ["https://*.myworkday.com/*"],
      "resources": [
        "config.json",
        "configParser.js",
        "replace/helpers.js",
        "replace/listNames.js",
        "replace/nsidNames.js",
        "replace/tableNames.js"
      ]
    },
    {
      "matches": ["https://*.appiancloud.com/*"],
      "resources": [
        "config.json",
        "configParser.js",
        "replace/helpers.js",
        "replace/listNamesAppian.js",
        "replace/sidnNames.js",
        "replace/tableNames.js"
      ]
    }
  ]
}